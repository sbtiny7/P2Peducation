<div id="category" class="panel panel-default">
  <div class="container panel-body">
    <div>
      <h2>Helpouts by category</h2>
    </div>

    <div id="category-list">
      <span class="glyphicon glyphicon-headphones" data-toggle="toggletip"
        data-placement="top" title="Music"></span>
      <span class="glyphicon glyphicon-book" data-toggle="toggletip"
        data-placement="top" title="Book"></span>
      <span class="glyphicon glyphicon-heart" data-toggle="toggletip"
        data-placement="top" title="Heart"></span>
      <span class="glyphicon glyphicon-star" data-toggle="toggletip"
        data-placement="top" title="Star"></span>
      <span class="glyphicon glyphicon-camera" data-toggle="toggletip"
        data-placement="top" title="Camera"></span>
    </div>

    <div class="row">
      <a href="#"><span class="left font-2x">MUSIC</span></a>
      <a href="#"><span class="right font-2x">VIEW ALL</span></a>
    </div>

    <% 2.times do %>
      <div class="row">
        <% 3.times do %>
          <div class="category-helpout col-lg-3">
            <div class="category-helpout-course bg-image-sm">
              <span>Cooking</span>
            </div>
            <a class="category-helpout-avatar" href="#">
              <div class="avatar-bg category-avatar">
                <%= image_tag "people.jpg", class: "avatar" %>
              </div>
              Alice
            </a>
            <div>
              <% 5.times do %>
                <span class="glyphicon glyphicon-star green"></span>
              <% end %>
              <span>ï¿¥19.99</span>
            </div>
          </div>
        <% end %>
      </div>
    <% end %>

  </div>
</div>
